                data.quotes.map(({ text, name, title }) => (
                  <li class="bg-midnight">
                    <div class="group aspect-square flex flex-col justify-end p-[1.875rem] bg-white/5 duration-300 ease-in-out overflow-hidden hover:bg-304">
                      <blockquote
                        class="text-[2.5rem] xs:text-[3rem] sm:text-[2.5rem] xl:text-[3rem] font-serif tracking-[-0.04em] leading-[1] group-hover:text-midnight duration-300 ease-in-out"
                        set:text={text}
                      />
                      {(name || title) && (
                        <div class="[interpolate-size:allow-keywords] opacity-0 h-0 group-hover:h-max group-hover:opacity-100 duration-300 ease-in-out">
                          <p
                            class="text-midnight pt-[1.6875rem] leading-[1.8125rem] text-[1.5rem] font-bold tracking-[-0.04em]"
                            set:html={[name, title].join('<br> ')}
                          />
                        </div>
                      )}
                    </div>
                  </li>
                ))
            }
          </ul>
        </div>
      </section>
      <section class="overflow-hidden">
        <div class="container pt-[3.75rem] pb-10 space-y-5 text-center">
          <hgroup
            class="text-[1.375rem] leading-[1.625rem] max-w-[60.3125rem] mx-auto text-midnight"
          >
            <h2 class="font-bold">{data.yearInReview.heading}</h2>
            <p>{data.yearInReview.description}</p>
          </hgroup>
          <ol class="space-y-5">
            {
              data.moments.map(({ date, heading }, index) => {
                const dir = resolve(`public/images/moments/${date}`)
                let images

                if (existsSync(dir)) {
                  images = readdirSync(dir)
                    .filter((image) => !image.startsWith('.'))
                    .map((image) => ({
                      src: `/images/moments/${date}/${image}`,
                    }))
                }

                return (
                  <li class="flex flex-col items-center gap-y-5">
                    {index > 0 && <div class="h-[12.25rem] w-px bg-gray-fog" />}
                    {(date || heading) && (
                      <h3
                        class="text-[3rem] leading-[1] tracking-[-0.04em] font-serif text-midnight"
                        set:html={[
                          new Intl.DateTimeFormat('en-US', {
                            timeZone: 'UTC',
                            month: 'long',
                            day: 'numeric',
                            year: 'numeric',
                          }).format(new Date(date)),
                          heading,
                        ].join('<br> ')}
                      />
                    )}
                    {images?.length ? (
                      <Pile class="z-10" images={images} />
                    ) : (
                      ''
                    )}
                  </li>
                )
              })
            }
          </ol>
        </div>
      </section>
      <Quotes {...data.testimonials} />
      <img
        src="/images/outro.webp"
        width="2880"
        height="1288"
        class="w-full"
        loading="lazy"
      />
      <section class="bg-blue">
        <div class="container text-white space-y-10 py-10 font-bold">
          <div>
            <p class="flex flex-col">
              <span
