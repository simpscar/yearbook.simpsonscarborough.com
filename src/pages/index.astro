<script>
  import Alpine from 'alpinejs'
  import { gsap } from 'gsap'
  import { ScrollTrigger } from 'gsap/ScrollTrigger'

  gsap.registerPlugin(ScrollTrigger)

  gsap
    .timeline({
      scrollTrigger: {
        trigger: '#map',
        start: 'top bottom',
        end: 'top top',
        scrub: 0,
      },
    })
    .from('#map circle', {
      opacity: 0,
      delay: () => Math.random(),
    })
    .from('#map g path', {
      opacity: 0,
      delay: () => Math.random(),
    })
</script>
